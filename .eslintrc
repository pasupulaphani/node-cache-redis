{
  "extends": "eslint:recommended",
  "env": {
    "browser": false,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "globals": {
    "describe": false,
    "before": false,
    "beforeEach": false,
    "afterEach": false,
    "it": false,
    "after": true,
    "window": true,
    "location": true,
    "document": true,
    "map": true,
    "navigator": true,
    "Promise": true
  },
  // http://eslint.org/docs/rules/
  "rules": {
    "camelcase": 1,
    "new-cap": 0,
    "key-spacing": 0,
    "no-multi-spaces": 0,
    "eol-last": 1,
    "strict": 0,
    "no-console": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-underscore-dangle": 0,
    "semi": [2, "always"],
    "space-before-function-paren": 1,
    "quotes": [2, "double", "avoid-escape"],
    "keyword-spacing": 1,
    "require-yield": 0,
    "valid-jsdoc": [1, {
      "prefer": {
          "return": "returns"
      }
    }]
  }
}
